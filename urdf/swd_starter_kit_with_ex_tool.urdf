<?xml version="1.0" encoding="utf-8"?>
<!-- This URDF describes the SWD Starter Kit robot -->

<robot name="swd_starter_kit">
  <link name="base_link">
    <visual>
      <origin xyz="-0.46 -0.27 0.0" rpy="0 0 0" />
      <geometry>
        <mesh
            filename="package://swd_starter_kit_description/meshes/starter_kit_base.stl"
            scale="0.001 0.001 0.001" />
      </geometry>

      <material name="">
        <color rgba="0.5 0.5 0.5 1" />
      </material>
    </visual>

    <collision>
      <origin xyz="-0.46 -0.27 0.0" rpy="0 0 0" />
      <geometry>
        <mesh
            filename="package://swd_starter_kit_description/meshes/starter_kit_base.stl"
            scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <!-- Calculations are made using a rectangular box of 540mm × 540mm × 151.5mm (L × W × H), as specified in the official documentation. -->
    <inertial>
      <origin xyz="-0.19 0 0.07575" rpy="0 0 0"/>
      <mass value="27"/>
      <inertia ixx="0.70774" ixy="0.0" ixz="0.0"
               iyy="0.70774" iyz="0.0"
               izz="1.31220" />
    </inertial>
  </link>

  <joint name="base_swd_left_joint" type="continuous">
    <parent link="base_link" />
    <child link="swd_left_link" />
    <origin xyz="0.0 0.25 0.08" rpy="-1.57 0 0" />
    <axis xyz="0 0 1" />
  </joint>

  <link name="swd_left_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <geometry>
        <mesh filename="package://swd_starter_kit_description/meshes/wheels/starter_kit_left_tire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.037" radius="0.0630"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1" />
      <inertia ixx="0.00111" ixy="0.0" ixz="0.0"
               iyy="0.00111" iyz="0.0"
               izz="0.00198" />
    </inertial>
  </link>

  <joint name="base_swd_right_joint" type="continuous">
    <parent link="base_link" />
    <child link="swd_right_link" />
    <origin xyz="0.0 -0.25 0.08" rpy="-1.57 0 0" />
    <axis xyz="0 0 1" />
  </joint>

  <link name="swd_right_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <geometry>
        <mesh filename="package://swd_starter_kit_description/meshes/wheels/starter_kit_right_tire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.037" radius="0.0630"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1" />
      <inertia ixx="0.00111" ixy="0.0" ixz="0.0"
               iyy="0.00111" iyz="0.0"
               izz="0.00198" />
    </inertial>
  </link>

  <joint name="caster_back_right_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_back_right_link"/>
    <origin xyz="-0.35 -0.17 0.025" rpy="0 0 0"/>
  </joint>

  <link name="caster_back_right_link">
    <collision>
      <origin xyz="0 0.001 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.005" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0"
               iyy="0.001" iyz="0.0"
               izz="0.001" />
    </inertial>
  </link>

  <joint name="caster_back_left_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_back_left_link"/>
    <origin xyz="-0.35 0.17 0.025" rpy="0 0 0"/>
  </joint>

  <link name="caster_back_left_link">
    <collision>
      <origin xyz="0 0.001 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.005" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0"
               iyy="0.001" iyz="0.0"
               izz="0.001" />
    </inertial>
  </link>

  <gazebo reference="swd_left_link">
    <mu1>0.7</mu1>
    <mu2>0.7</mu2>
    <kp>1000000.0</kp>
    <kd>10.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>0.1</maxVel>
    <fdir1>1 0 0</fdir1>
    <material>Gazebo/Yellow</material>
  </gazebo>

  <gazebo reference="swd_right_link">
    <mu1>0.7</mu1>
    <mu2>0.7</mu2>
    <kp>1000000.0</kp>
    <kd>10.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>0.1</maxVel>
    <fdir1>1 0 0</fdir1>
    <material>Gazebo/Yellow</material>
  </gazebo>

  <gazebo reference="caster_back_right_link">
    <mu1>0.05</mu1>
    <mu2>0.05</mu2>
    <kp>1000000.0</kp>
    <kd>10.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>1.0</maxVel>
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo reference="caster_back_left_link">
    <mu1>0.05</mu1>
    <mu2>0.05</mu2>
    <kp>1000000.0</kp>
    <kd>10.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>1.0</maxVel>
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo>
    <plugin name="diff_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <commandTopic>/cmd_vel</commandTopic>
      <odometryTopic>/odom</odometryTopic>
      <odometryFrame>map</odometryFrame>
      <odometrySource>world</odometrySource>
      <publishOdomTF>false</publishOdomTF>
      <robotBaseFrame>base_link</robotBaseFrame>
      <publishWheelTF>false</publishWheelTF>
      <publishTf>true</publishTf>
      <publishWheelJointState>true</publishWheelJointState>
      <legacyMode>false</legacyMode>
      <updateRate>30</updateRate>
      <leftJoint>base_swd_left_joint</leftJoint>
      <rightJoint>base_swd_right_joint</rightJoint>
      <wheelSeparation>0.485</wheelSeparation>
      <wheelDiameter>0.125</wheelDiameter>
      <wheelAcceleration>11</wheelAcceleration>
      <wheelTorque>37</wheelTorque>
      <rosDebugLevel>na</rosDebugLevel>
    </plugin>
  </gazebo>

  <joint name="base_lidar_link" type="fixed">
    <origin xyz="0.05 0 0.2" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="lidar_link" />
    <axis xyz="0 0 0" />
  </joint>

  <link name="lidar_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.095" radius="0.04"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.5" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.00058" ixy="0.0" ixz="0.0"
               iyy="0.00058" iyz="0.0"
               izz="0.00040" />
    </inertial>
  </link>

  <gazebo reference="lidar_link">
    <material>Gazebo/FlatBlack</material>
    <sensor type="ray" name="lidar_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>30</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>1080</samples>
            <resolution>1</resolution>
            <min_angle>-2.356194496154785</min_angle>
            <max_angle>2.356194496154785</max_angle>
          </horizontal>
        </scan>

        <range>
          <min>0.09</min>
          <max>20</max>
          <resolution>0.001</resolution>
        </range>

        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>

      <plugin name="laser_controller" filename="libgazebo_ros_laser.so">
        <topicName>scan</topicName>
        <frameName>lidar_link</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <joint name="base_imu_link" type="fixed">
    <origin xyz="-0.05 0 0.2" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="imu_sensor_link" />
    <axis xyz="0 0 0" />
  </joint>

  <link name="imu_sensor_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.055 0.0368 0.024"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.00002" ixy="0.0" ixz="0.0"
               iyy="0.00003" iyz="0.0"
               izz="0.00004" />
    </inertial>
  </link>
  
  <gazebo reference="imu_sensor_link">
    <material>Gazebo/FlatBlack</material>
    <sensor type="imu" name="imu_sensor">
      <pose>0 0 0 0 0 0</pose>
      <always_on>true</always_on>
      <update_rate>200</update_rate>
      <visualize>false</visualize>
      <plugin name="imu_controller" filename="libgazebo_ros_imu_sensor.so">
        <topicName>imu</topicName>
        <bodyName>imu_sensor_link</bodyName>
        <updateRateHZ>200.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu_link</frameName>
        <initialOrientationAsReference>false</initialOrientationAsReference>
      </plugin>
    </sensor>
  </gazebo>
</robot>


